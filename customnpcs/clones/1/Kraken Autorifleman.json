    "Scripts": [
        {
            "Script": "var Animation = 1;

function target(t) {
Animation = Math.floor(Math.random() * (3 - 1 + 1) + 1);

      
    if(Animation == 1) {
        t.npc.ai.setAnimation(0);
        t.npc.stats.ranged.setHasAimAnimation(true) ; 
    }
    
    if(Animation == 2) {
        t.npc.ai.setAnimation(4);
        t.npc.stats.ranged.setHasAimAnimation(false);
    }
    
    if(Animation == 3) {
    t.npc.ai.setAnimation(7);
    t.npc.stats.ranged.setHasAimAnimation(false);
    }
    
}",
            "Console": [
            ],
            "ScriptList": [
            ]
        },
        {
            "Script": "var LIMIT = 10; //Projectile limit before running way
var TacticalType;
var ShootAt = 0;
function tick (t) {
var Flyby = t.npc.world.getNearbyEntities(t.npc.getPos(), 15, EntityType_PROJECTILE);

if (Flyby.length > 1) {
    ShootAt++
    if(ShootAt >= LIMIT) {
        TacticalType = t.npc.ai.getTacticalType();
        t.npc.ai.setRetaliateType(2);
        t.npc.timers.forceStart(678, 200, false);
    }
}

}

function timer(o) {
    o.npc.ai.setRetaliateType(0);
    o.npc.ai.setTacticalType(TacticalType);
    ShootAt = 0;
}",
            "Console": [
            ],
            "ScriptList": [
            ]
        },
        {
            "Script": "var target;
var targetPos;
var fireStop;
function target(e) {
   target = e.entity;
   targetPos = target.getPos();
}

function targetLost(e) {
    while (fireStop == false) {
    var targetX = targetPos.x;
    var targetY = targetPos.y;
    var targetZ = targetPos.z;
    var bulletCount = e.npc.stats.ranged.getShotCount();
    
    for (var i = 0; i < bulletCoun; i++) {
        e.npc.shootItem(Math.random() * targetX, Math.random() * targetY, Math.random() * targetZ, e.npc.inventory.getProjectile(), e.npc.stats.ranged.getAccuracy())
        }
    }
}",
