    "Scripts": [
        {
            "Script": "var Animation = 1;

function target(t) {
Animation = Math.floor(Math.random() * (3 - 1 + 1) + 1);

      
    if(Animation == 1) {
        t.npc.ai.setAnimation(0);
        t.npc.stats.ranged.setHasAimAnimation(true) ; 
    }
    
    if(Animation == 2) {
        t.npc.ai.setAnimation(4);
        t.npc.stats.ranged.setHasAimAnimation(false);
    }
    
    if(Animation == 3) {
    t.npc.ai.setAnimation(7);
    t.npc.stats.ranged.setHasAimAnimation(false);
    }
    
}",
            "Console": [
            ],
            "ScriptList": [
            ]
        },
        {
            "Script": "var LIMIT = 12; //Projectile limit before running way
var TacticalType;
var ShootAt = 0;
function tick (t) {
var Flyby = t.npc.world.getNearbyEntities(t.npc.getPos(), 15, EntityType_PROJECTILE);

if (Flyby.length > 1) {
    ShootAt++
    if(ShootAt >= LIMIT) {
        TacticalType = t.npc.ai.getTacticalType();
        t.npc.ai.setRetaliateType(2);
        t.npc.timers.forceStart(678, 200, false);
    }
}

}

function timer(o) {
    o.npc.ai.setRetaliateType(0);
    o.npc.ai.setTacticalType(TacticalType);
    ShootAt = 0;
}",
